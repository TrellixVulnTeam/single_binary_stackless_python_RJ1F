
MINI_PYTHON = ..\..\python.exe

ProjectDir = ..
externalsDir = $(ProjectDir)\externals

!include common_arch.mak

!IF "$(BUILD_TARGET_CPU)" == "x86"
INCLUDES = /I "$(ProjectDir)\externals\openssl-1.0.2k\include32" /I "$(ProjectDir)\Include" /I "$(ProjectDir)\PC" /I "$(ProjectDir)\Stackless"
!ELSE
INCLUDES = /I "$(ProjectDir)\externals\openssl-1.0.2k\include64" /I "$(ProjectDir)\Include" /I "$(ProjectDir)\PC" /I "$(ProjectDir)\Stackless"
!ENDIF
DEFINES = /D "Py_BUILD_CORE" /D "Py_ENABLE_SHARED" /D "WIN32" /D "NDEBUG"
CFLAGS = /O2 /Ob1 /Oi /GL $(INCLUDES) $(DEFINES) $(ARCH_DEFINES) /GF /FD /MT /Gy /W3 /nologo /Zi
LFLAGS = /NOLOGO /LTCG
TARGET = _hashlib.lib


OBJS = $(ProjectDir)\Modules\_hashopenssl.obj

!include common.mak
