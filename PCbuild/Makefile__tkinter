
ProjectDir = ..
externalsDir = $(ProjectDir)\externals

!include common_arch.mak

INCLUDES = /I "$(externalsDir)\tcltk\include" /I "$(ProjectDir)\Include" /I "$(ProjectDir)\PC" /I "$(ProjectDir)\Stackless"
DEFINES = /D "MODULE_NAME=\"tkinter\"" /D "WITH_APPINIT" /D "Py_BUILD_CORE" /D "Py_ENABLE_SHARED" /D "WIN32" /D "NDEBUG"
CFLAGS = /O2 /Ob1 /Oi /GL $(INCLUDES) $(DEFINES) $(ARCH_DEFINES) /GF /FD /MT /Gy /W3 /nologo /Zi
LFLAGS = /NOLOGO /LTCG
TARGET = _tkinter.lib


OBJS = $(ProjectDir)\Modules\_tkinter.obj \
          $(ProjectDir)\Modules\tkappinit.obj

!include common.mak
