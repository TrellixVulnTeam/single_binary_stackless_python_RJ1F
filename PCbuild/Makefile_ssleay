
BASE_LIB = ssleay_$(BUILD_TARGET_CPU)_vc$(VC_VERSION).lib
ProjectDir = ..
externalsDir = $(ProjectDir)\externals
opensslDir = $(externalsDir)\openssl-1.0.2k

all:
	copy /Y $(BASE_LIB) ssleay.lib

clean:
	-del ssleay.lib

