
ProjectDir = ..
externalsDir = $(ProjectDir)\externals
sqlite3Dir = $(externalsDir)\sqlite-3.8.11.0

!include common_arch.mak

INCLUDES = /I "$(externalsDir)\sqlite-3.8.11.0" /I "$(ProjectDir)\Include" /I "$(ProjectDir)\PC" /I "$(ProjectDir)\Stackless"
DEFINES = /D "SQLITE_API=" /D "NDEBUG" /D "WIN32"
CFLAGS = /O2 /Ob1 /Oi /GL $(INCLUDES) $(DEFINES) $(ARCH_DEFINES) /GF /FD /MT /Gy /W3 /nologo /Zi
LFLAGS = /NOLOGO /LTCG
TARGET = sqlite3.lib


OBJS = $(sqlite3Dir)\sqlite3.obj

!include common.mak
